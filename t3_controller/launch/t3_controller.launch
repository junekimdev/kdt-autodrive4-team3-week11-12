  <!-- processor -->
  <node name="lane_cam" pkg="t3_sensor_lane_cam" type="t3_sensor_lane_cam" output="screen"/>
  <node name="controller" pkg="t3_controller" type="t3_controller" output="screen"/>
</launch>
